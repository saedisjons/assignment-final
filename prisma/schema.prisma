// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Game {
  id           String   @id @default(uuid())
  createdAt    DateTime @default(now())
  moves        String[]
  player1_name String
  player2_name String?
}
